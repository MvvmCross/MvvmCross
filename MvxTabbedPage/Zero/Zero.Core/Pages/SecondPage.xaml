<?xml version="1.0" encoding="utf-8" ?>
<mvx:MvxContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Zero.Core.Pages.SecondPage"
             xmlns:res="clr-namespace:Zero.Core.Resources;assembly=Zero.Core"
             xmlns:mvx="clr-namespace:MvvmCross.Forms.Views;assembly=MvvmCross.Forms"
             Title="Second">
  <StackLayout>
    <Entry Text="{Binding SuperNumber}"/>
    <Label Text="{Binding MainPageButtonText}"/>
    <Button Text="Back" Command="{Binding BackCommand}" />
  </StackLayout>
</mvx:MvxContentPage>