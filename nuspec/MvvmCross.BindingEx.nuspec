<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
	<metadata>
		<id>MvvmCross.BindingEx</id>
		<version>4.0.0-beta7</version>
		<title>MvvmCross - Binding Extensions - for Windows only</title>
		<authors>MvvmCross</authors>
		<owners>Stuart Lodge</owners>
		<licenseUrl>http://opensource.org/licenses/ms-pl.html</licenseUrl>
		<projectUrl>https://github.com/MvvmCross/MvvmCross</projectUrl>
		<requireLicenseAcceptance>false</requireLicenseAcceptance>
		<description>
		MvvmCross is the .NET MVVM framework for cross-platform solutions, including Xamarin iOS, Xamarin Android, Xamarin Forms, Windows and Mac.

		This package contains the 'binding extensions' for MvvmCross
		</description>
		<tags>mvvm mvvmcross cross xamarin android ios forms monodroid monotouch xamarin.android xamarin.ios xamarin.forms wpf windows8 winrt net net45 netcore wp wpdev windowsphone windowsstore uwp</tags>
		<iconUrl>http://i.imgur.com/BvdAtgT.png</iconUrl>
		<dependencies>
			<group targetFramework ="net">
				<dependency id="MvvmCross.HotTuna.MvvmCrossLibraries" version="4.0.0-beta7" />
				<dependency id="MvvmCross.HotTuna.Binding" version="4.0.0-beta7" />
			</group>
			<group targetFramework ="win">
				<dependency id="MvvmCross.HotTuna.MvvmCrossLibraries" version="4.0.0-beta7" />
				<dependency id="MvvmCross.HotTuna.Binding" version="4.0.0-beta7" />
			</group>
			<group targetFramework ="wp">
				<dependency id="MvvmCross.HotTuna.MvvmCrossLibraries" version="4.0.0-beta7" />
				<dependency id="MvvmCross.HotTuna.Binding" version="4.0.0-beta7" />
			</group>
			<group targetFramework ="wpa">
				<dependency id="MvvmCross.HotTuna.MvvmCrossLibraries" version="4.0.0-beta7" />
				<dependency id="MvvmCross.HotTuna.Binding" version="4.0.0-beta7" />
			</group>
			<group targetFramework ="MonoAndroid">
				<dependency id="MvvmCross.HotTuna.MvvmCrossLibraries" version="4.0.0-beta7" />
				<dependency id="MvvmCross.HotTuna.Binding" version="4.0.0-beta7" />
			</group>
			<group targetFramework ="MonoTouch">
				<dependency id="MvvmCross.HotTuna.MvvmCrossLibraries" version="4.0.0-beta7" />
				<dependency id="MvvmCross.HotTuna.Binding" version="4.0.0-beta7" />
			</group>
			<group targetFramework ="Xamarin.iOS">
				<dependency id="MvvmCross.HotTuna.MvvmCrossLibraries" version="4.0.0-beta7" />
				<dependency id="MvvmCross.HotTuna.Binding" version="4.0.0-beta7" />
			</group>
			<group targetFramework ="portable-net45+win8+wpa81+wp8">
				<dependency id="MvvmCross.HotTuna.MvvmCrossLibraries" version="4.0.0-beta7" />
				<dependency id="MvvmCross.HotTuna.Binding" version="4.0.0-beta7" />
			</group>
			<group targetFramework ="uap">
				<dependency id="MvvmCross.HotTuna.MvvmCrossLibraries" version="4.0.0-beta7" />
				<dependency id="MvvmCross.HotTuna.Binding" version="4.0.0-beta7" />
			</group>
			<group targetFramework ="dotnet">
				<dependency id="MvvmCross.HotTuna.MvvmCrossLibraries" version="4.0.0-beta7" />
				<dependency id="MvvmCross.HotTuna.Binding" version="4.0.0-beta7" />
			</group>
		</dependencies>
	</metadata>
	<files>		

		<!-- common -->
		<file src="..\Cirrious\Cirrious.MvvmCross.BindingEx.Wpf\**\*.cs" 
			target="src\Cirrious.MvvmCross.BindingEx.Wpf" />
		<file src="..\Cirrious\Cirrious.MvvmCross.BindingEx.WindowsStore\**\*.cs" 
			target="src\Cirrious.MvvmCross.BindingEx.WindowsStore" />
		<file src="..\Cirrious\Cirrious.MvvmCross.BindingEx.WindowsPhone\**\*.cs" 
			target="src\Cirrious.MvvmCross.BindingEx.WindowsPhone" />

		<!-- wpf -->
		<file src="..\bin\Release\Mvx\Wpf\Cirrious.MvvmCross.BindingEx.Wpf.*" target="lib\net45" />

		<!-- store -->
		<file src="..\bin\Release\Mvx\WindowsStore\Cirrious.MvvmCross.BindingEx.WindowsStore.*" target="lib\netcore45" />

		<!-- phone -->
		<file src="..\bin\Release\Mvx\WindowsPhone\Cirrious.MvvmCross.BindingEx.WindowsPhone.*" target="lib\wp8" />
	</files>
</package>








