<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
	<metadata>
		<id>MvvmCross.HotTuna.Plugin.Json</id>
		<version>4.0.0-beta5</version>
		<title>MvvmCross - Json Plugin</title>
		<authors>MvvmCross</authors>
		<owners>Stuart Lodge</owners>
		<licenseUrl>http://opensource.org/licenses/ms-pl.html</licenseUrl>
		<projectUrl>https://github.com/MvvmCross/MvvmCross-Plugins</projectUrl>
		<requireLicenseAcceptance>false</requireLicenseAcceptance>
		<description>This package contains the 'Json' plugin for MvvmCross</description>
		<tags>mvvm mvvmcross xamarin monodroid Xamarin.iOS10 wpf windows8 winrt netcore wp wpdev windowsphone</tags>
		<iconUrl>http://i.imgur.com/BvdAtgT.png</iconUrl>
		<dependencies>
			<group targetFramework ="net">
				<dependency id="MvvmCross.HotTuna.CrossCore" version="4.0.0-beta5" />
				<dependency id="Newtonsoft.Json" version="7.0.1" />
			</group>
			<group targetFramework ="win">
				<dependency id="MvvmCross.HotTuna.CrossCore" version="4.0.0-beta5" />
				<dependency id="Newtonsoft.Json" version="7.0.1" />
			</group>
			<group targetFramework ="wp">
				<dependency id="MvvmCross.HotTuna.CrossCore" version="4.0.0-beta5" />
				<dependency id="Newtonsoft.Json" version="7.0.1" />
			</group>
			<group targetFramework ="wpa">
				<dependency id="MvvmCross.HotTuna.CrossCore" version="4.0.0-beta5" />
				<dependency id="Newtonsoft.Json" version="7.0.1" />
			</group>
			<group targetFramework ="MonoAndroid">
				<dependency id="MvvmCross.HotTuna.CrossCore" version="4.0.0-beta5" />
				<dependency id="Newtonsoft.Json" version="7.0.1" />
			</group>
			<group targetFramework ="MonoTouch">
				<dependency id="MvvmCross.HotTuna.CrossCore" version="4.0.0-beta5" />
				<dependency id="Newtonsoft.Json" version="7.0.1" />
			</group>
			<group targetFramework ="Xamarin.iOS">
				<dependency id="MvvmCross.HotTuna.CrossCore" version="4.0.0-beta5" />
				<dependency id="Newtonsoft.Json" version="7.0.1" />
			</group>
			<group targetFramework ="portable-net45+win8+wpa81+wp8">
				<dependency id="MvvmCross.HotTuna.CrossCore" version="4.0.0-beta5" />
				<dependency id="Newtonsoft.Json" version="7.0.1" />
			</group>
			<group targetFramework ="uap">
				<dependency id="MvvmCross.HotTuna.CrossCore" version="4.0.0-beta5" />
				<dependency id="Newtonsoft.Json" version="7.0.1" />
			</group>
			<group targetFramework ="dotnet">
				<dependency id="MvvmCross.HotTuna.CrossCore" version="4.0.0-beta5" />
				<dependency id="Newtonsoft.Json" version="7.0.1" />
			</group>
		</dependencies>
	</metadata>
	<files>
		<!-- Common -->
		<file src="..\Json\MvvmCross.Plugins.Json\**\*.cs" target="src\MvvmCross.Plugins.Json" />

		<!-- PCL -->
		<file src="..\bin\Release\Mvx\Portable\MvvmCross.Plugins.Json.*" target="lib\portable-win+net45+wp8+win8+wpa81" />

		<!-- dotnet -->
		<file src="..\bin\Release\Mvx\Portable\MvvmCross.Plugins.Json.*" target="lib\dotnet" />

		<!-- wpf -->
		<file src="..\bin\Release\Mvx\Portable\MvvmCross.Plugins.Json.*" target="lib\net45" />
		<file src="BootstrapContent\JsonPluginBootstrap.cs.pp" target="content\net45\Bootstrap\JsonPluginBootstrap.cs.pp" />

		<!-- store -->
		<file src="..\bin\Release\Mvx\Portable\MvvmCross.Plugins.Json.*" target="lib\netcore45" />
		<file src="BootstrapContent\JsonPluginBootstrap.cs.pp" target="content\netcore45\Bootstrap\JsonPluginBootstrap.cs.pp" />

		<!-- phone -->
		<file src="..\bin\Release\Mvx\Portable\MvvmCross.Plugins.Json.*" target="lib\wp8" />
		<file src="BootstrapContent\JsonPluginBootstrap.cs.pp" target="content\wp8\Bootstrap\JsonPluginBootstrap.cs.pp" />

		<!-- windows common -->
		<file src="..\bin\Release\Mvx\Portable\MvvmCross.Plugins.Json.*" target="lib\win81" />
		<file src="..\bin\Release\Mvx\Portable\MvvmCross.Plugins.Json.*" target="lib\wpa81" />
		<file src="..\bin\Release\Mvx\Portable\MvvmCross.Plugins.Json.*" target="lib\portable-win81+wpa81" />	
		<file src="BootstrapContent\JsonPluginBootstrap.cs.pp" target="content\win81\Bootstrap\JsonPluginBootstrap.cs.pp" />	
		<file src="BootstrapContent\JsonPluginBootstrap.cs.pp" target="content\wpa81\Bootstrap\JsonPluginBootstrap.cs.pp" />	

		<!-- droid -->
		<file src="..\bin\Release\Mvx\Portable\MvvmCross.Plugins.Json.*" target="lib\MonoAndroid" />
		<file src="BootstrapContent\JsonPluginBootstrap.cs.pp" target="content\MonoAndroid\Bootstrap\JsonPluginBootstrap.cs.pp" />

		<!-- touch -->
		<file src="..\bin\Release\Mvx\Portable\MvvmCross.Plugins.Json.*" target="lib\Xamarin.iOS10" />	
		<file src="TouchBootstrapContent\JsonPluginBootstrap.cs.pp" target="content\Xamarin.iOS10\Bootstrap\JsonPluginBootstrap.cs.pp" />
	</files>
</package>








