<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
	<metadata>
		<id>MvvmCross.HotTuna.Mac.MvvmCrossLibraries</id>
		<version>3.1.1-beta1</version>
		<title>MvvmCross - Hot Tuna Mac MvvmCross Libraries</title>
		<authors>MvvmCross</authors>
		<owners>Stuart Lodge</owners>
		<requireLicenseAcceptance>false</requireLicenseAcceptance>
		<description>This package contains the Mac 'MvvmCross Library' assemblies for MvvmCross v3 - Hot Tuna. For convenience, PCL, MonoAndroid and MonoTouch are included. This and other packages are currently under development...</description>
		<dependencies>
		  <dependency id="MvvmCross.HotTuna.Mac.CrossCore" version="3.1.1-beta1" />
		</dependencies>
	</metadata>
	<files>		
		<!-- PCL -->
		<file src="..\bin\Release\Mvx\Portable\Cirrious.MvvmCross.dll" target="lib\portable-win+net45+sl50+wp8+MonoAndroid+MonoTouch\Cirrious.MvvmCross.dll" />

		<!-- droid -->
		<file src="..\bin\Release\Mvx\Droid\Cirrious.MvvmCross.Droid.dll" target="lib\MonoAndroid\Cirrious.MvvmCross.Droid.dll" />

		<!-- droid - PCL -->
		<file src="..\bin\Release\Mvx\Portable\Cirrious.MvvmCross.dll" target="lib\MonoAndroid\Cirrious.MvvmCross.dll" />
		
		<!-- touch -->
		<file src="..\bin\Release\Mvx\Touch\Cirrious.MvvmCross.Touch.dll" target="lib\MonoTouch\Cirrious.MvvmCross.Touch.dll" />

		<!-- touch - PCL -->
		<file src="..\bin\Release\Mvx\Portable\Cirrious.MvvmCross.dll" target="lib\MonoTouch\Cirrious.MvvmCross.dll" />

		<!-- mac -->
		<file src="..\bin\Release\Mvx\Mac\Cirrious.MvvmCross.Mac.dll" target="lib\net45\Cirrious.MvvmCross.Mac.dll" />

		<!-- mac - PCL -->
		<file src="..\bin\Release\Mvx\Portable\Cirrious.MvvmCross.dll" target="lib\net45\Cirrious.MvvmCross.dll" />

		
	</files>
</package>