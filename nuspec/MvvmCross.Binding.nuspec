<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
	<metadata>
		<id>MvvmCross.Binding</id>
		<version>$version$</version>
		<title>MvvmCross - Binding</title>
		<description>
		MvvmCross is the .NET MVVM framework for cross-platform solutions, including Xamarin iOS, Xamarin Android, Xamarin Forms, Windows and Mac.

This package contains the 'Core Binding' libraries for MvvmCross
		</description>
		<tags>mvvm mvvmcross cross xamarin android ios forms monodroid monotouch xamarin.android xamarin.ios xamarin.forms wpf net net45 netcore uwp</tags>
		<iconUrl>http://i.imgur.com/BvdAtgT.png</iconUrl>
		<frameworkAssemblies>
			<frameworkAssembly assemblyName="Mono.Android.Export" targetFramework="MonoAndroid" />
		</frameworkAssemblies>
		<dependencies>
			<dependency id="MvvmCross.Core" version="[$version$]" />
		</dependencies>
	</metadata>
	<files>		
		<!-- pcl -->
		<file src="..\bin\Release\Mvx\Portable\MvvmCross.Binding.*" target="lib\portable-net45+win+wpa81+wp80" />
		<file src="..\bin\Release\Mvx\Portable\MvvmCross.Localization.*" target="lib\portable-net45+win+wpa81+wp80" />
		
		<!-- dotnet -->
		<file src="..\bin\Release\Mvx\Portable\MvvmCross.Binding.*" target="lib\dotnet" />
		<file src="..\bin\Release\Mvx\Portable\MvvmCross.Localization.*" target="lib\dotnet" />
		
		<!-- droid -->
		<file src="..\bin\Release\Mvx\Droid\MvvmCross.Binding.Droid.*" target="lib\MonoAndroid" />
		<file src="..\bin\Release\Mvx\Portable\MvvmCross.Binding.*" target="lib\MonoAndroid" />
		<file src="..\bin\Release\Mvx\Portable\MvvmCross.Localization.*" target="lib\MonoAndroid" />

		<!-- iOS -->
		<file src="..\bin\Release\Mvx\iOS\MvvmCross.Binding.iOS.*" target="lib\Xamarin.iOS10" />
		<file src="..\bin\Release\Mvx\Portable\MvvmCross.Binding.*" target="lib\Xamarin.iOS10" />
		<file src="..\bin\Release\Mvx\Portable\MvvmCross.Localization.*" target="lib\Xamarin.iOS10" />

		<!-- mac -->
		<file src="..\bin\Release\Mvx\Mac\MvvmCross.Binding.Mac.*" target="lib\Xamarin.Mac20" />
		<file src="..\bin\Release\Mvx\Portable\MvvmCross.Binding.*" target="lib\Xamarin.Mac20" />
		<file src="..\bin\Release\Mvx\Portable\MvvmCross.Localization.*" target="lib\Xamarin.Mac20" />
		
		<!-- tvOS -->
		<file src="..\bin\Release\Mvx\tvOS\MvvmCross.Binding.tvOS.*" target="lib\Xamarin.tvOS" />
		<file src="..\bin\Release\Mvx\Portable\MvvmCross.Binding.*" target="lib\Xamarin.tvOS" />
		<file src="..\bin\Release\Mvx\Portable\MvvmCross.Localization.*" target="lib\Xamarin.tvOS" />

		<!-- wpf -->
		<file src="..\bin\Release\Mvx\Wpf\MvvmCross.Binding.Wpf.*" target="lib\net45" />
		<file src="..\bin\Release\Mvx\Portable\MvvmCross.Binding.*" target="lib\net45" />
		<file src="..\bin\Release\Mvx\Portable\MvvmCross.Localization.*" target="lib\net45" />

		<!-- uwp -->
		<file src="..\bin\Release\Mvx\Uwp\MvvmCross.Binding.Uwp.*" target="lib\uap10.0" />
		<file src="..\bin\Release\Mvx\Portable\MvvmCross.Binding.*" target="lib\uap10.0" />
		<file src="..\bin\Release\Mvx\Portable\MvvmCross.Localization.*" target="lib\uap10.0" />
	</files>
</package>