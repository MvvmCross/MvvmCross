			</group>
			<group targetFramework="Xamarin.Mac20">		
				<dependency id="MvvmCross.Core" version="4.3.0" />
 			</group>
		</dependencies>
	</metadata>
	<files>		
		<!-- pcl -->
		<file src="..\bin\Release\Mvx\Portable\MvvmCross.Binding.*" target="lib\portable-net45+win+wpa81+wp80" />
