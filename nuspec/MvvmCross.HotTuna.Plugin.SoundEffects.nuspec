		<requireLicenseAcceptance>false</requireLicenseAcceptance>
		<description>This package contains the 'SoundEffects' plugin for MvvmCross</description>
		<tags>mvvm mvvmcross xamarin monodroid Xamarin.iOS10 wpf windows8 winrt netcore wp wpdev windowsphone</tags>
		<iconUrl>http://i.imgur.com/ymzMpLX.png</iconUrl>
		<dependencies>
			<group targetFramework ="net">
				<dependency id="MvvmCross.HotTuna.CrossCore" version="4.0.0-beta3" />
			</group>
			<group targetFramework ="win">
				<dependency id="MvvmCross.HotTuna.CrossCore" version="4.0.0-beta3" />
			</group>
			<group targetFramework ="wp">
				<dependency id="MvvmCross.HotTuna.CrossCore" version="4.0.0-beta3" />
			</group>
			<group targetFramework ="wpa">
				<dependency id="MvvmCross.HotTuna.CrossCore" version="4.0.0-beta3" />
			</group>
			<group targetFramework ="MonoAndroid">
				<dependency id="MvvmCross.HotTuna.CrossCore" version="4.0.0-beta3" />
			</group>
			<group targetFramework ="MonoTouch">
				<dependency id="MvvmCross.HotTuna.CrossCore" version="4.0.0-beta3" />
