	<files>
		<!-- src -->
		<file src="..\Network\MvvmCross.Plugins.Network\**\*.cs" 
			target="src\MvvmCross.Plugins.Network" />
		<file src="..\Network\MvvmCross.Plugins.Network.Wpf\**\*.cs" 
