<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
	<metadata>
		<id>MvvmCross.HotTuna.Plugin.DownloadCache</id>
		<version>4.0.0-beta4</version>
		<title>MvvmCross - DownloadCache Plugin</title>
		<authors>MvvmCross</authors>
		<owners>Stuart Lodge</owners>
		<licenseUrl>http://opensource.org/licenses/ms-pl.html</licenseUrl>
		<projectUrl>https://github.com/MvvmCross/MvvmCross-Plugins</projectUrl>
		<requireLicenseAcceptance>false</requireLicenseAcceptance>
		<description>This package contains the 'DownloadCache' plugin for MvvmCross</description>
		<tags>mvvm mvvmcross xamarin monodroid Xamarin.iOS10 wpf windows8 winrt netcore wp wpdev windowsphone</tags>
		<iconUrl>http://i.imgur.com/BvdAtgT.png</iconUrl>
		<dependencies>
			<group targetFramework ="net">
				<dependency id="MvvmCross.HotTuna.CrossCore" version="4.0.0-beta4" />
			</group>
			<group targetFramework ="win">
				<dependency id="MvvmCross.HotTuna.CrossCore" version="4.0.0-beta4" />
			</group>
			<group targetFramework ="wp">
				<dependency id="MvvmCross.HotTuna.CrossCore" version="4.0.0-beta4" />
			</group>
			<group targetFramework ="wpa">
				<dependency id="MvvmCross.HotTuna.CrossCore" version="4.0.0-beta4" />
			</group>
			<group targetFramework ="MonoAndroid">
				<dependency id="MvvmCross.HotTuna.CrossCore" version="4.0.0-beta4" />
				<dependency id="Xamarin.Android.Support.v4" version="23.0.1.3" />
			</group>
			<group targetFramework ="MonoTouch">
				<dependency id="MvvmCross.HotTuna.CrossCore" version="4.0.0-beta4" />
			</group>
			<group targetFramework ="Xamarin.iOS">
				<dependency id="MvvmCross.HotTuna.CrossCore" version="4.0.0-beta4" />
			</group>
			<group targetFramework ="portable-net45+win8+wpa81+wp8">
				<dependency id="MvvmCross.HotTuna.CrossCore" version="4.0.0-beta4" />
			</group>
			<group targetFramework ="uap">
				<dependency id="MvvmCross.HotTuna.CrossCore" version="4.0.0-beta4" />
			</group>
			<group targetFramework ="dotnet">
				<dependency id="MvvmCross.HotTuna.CrossCore" version="4.0.0-beta4" />
			</group>
		</dependencies>
	</metadata>
	<files>		
		<!-- src -->
		<file src="..\DownloadCache\MvvmCross.Plugins.DownloadCache\**\*.cs" 
			target="src\MvvmCross.Plugins.DownloadCache" />
		<file src="..\DownloadCache\MvvmCross.Plugins.DownloadCache.Droid\**\*.cs" 
			target="src\MvvmCross.Plugins.DownloadCache.Droid" />
		<file src="..\DownloadCache\MvvmCross.Plugins.DownloadCache.Touch\**\*.cs" 
			target="src\MvvmCross.Plugins.DownloadCache.Touch" />	

		<!-- PCL -->
		<file src="..\bin\Release\Mvx\Portable\MvvmCross.Plugins.DownloadCache.*" target="lib\portable-win+net45+wp8+win8+wpa81" />

		<!-- dotnet -->
		<file src="..\bin\Release\Mvx\Portable\MvvmCross.Plugins.DownloadCache.*" target="lib\dotnet" />

		<!-- droid -->
		<file src="..\bin\Release\Mvx\Portable\MvvmCross.Plugins.DownloadCache.*" target="lib\MonoAndroid" />
		<file src="..\bin\Release\Mvx\Droid\MvvmCross.Plugins.DownloadCache.Droid.*" target="lib\MonoAndroid" />
		<file src="BootstrapContent\DownloadCachePluginBootstrap.cs.pp" target="content\MonoAndroid\Bootstrap\DownloadCachePluginBootstrap.cs.pp" />

		<!-- touch -->
		<file src="..\bin\Release\Mvx\Portable\MvvmCross.Plugins.DownloadCache.*" target="lib\Xamarin.iOS10" />
		<file src="..\bin\Release\Mvx\Touch\MvvmCross.Plugins.DownloadCache.Touch.*" target="lib\Xamarin.iOS10" />
		<file src="TouchBootstrapContent\DownloadCachePluginBootstrap.cs.pp" target="content\Xamarin.iOS10\Bootstrap\DownloadCachePluginBootstrap.cs.pp" />

	</files>
</package>







