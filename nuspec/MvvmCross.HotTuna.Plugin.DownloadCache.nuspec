<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
	<metadata>
		<id>MvvmCross.HotTuna.Plugin.DownloadCache</id>
		<version>4.0.0-alpha8</version>
		<title>MvvmCross - DownloadCache Plugin</title>
		<authors>MvvmCross</authors>
		<owners>Stuart Lodge</owners>
		<licenseUrl>http://opensource.org/licenses/ms-pl.html</licenseUrl>
		<projectUrl>https://github.com/slodge/MvvmCross</projectUrl>
		<requireLicenseAcceptance>false</requireLicenseAcceptance>
		<description>This package contains the 'DownloadCache' plugin for MvvmCross v3 - Hot Tuna. This and other packages are currently under development...</description>
		<tags>mvvm mvvmcross xamarin monodroid Xamarin.iOS10 wpf windows8 winrt netcore wp wpdev windowsphone</tags>
		<dependencies>
		  <dependency id="MvvmCross.HotTuna.CrossCore" version="4.0.0-alpha8" />
		</dependencies>
		<iconUrl>http://i.imgur.com/ymzMpLX.png</iconUrl>
	</metadata>
	<files>		
		<!-- src -->
		<file src="..\Plugins\Cirrious\DownloadCache\Cirrious.MvvmCross.Plugins.DownloadCache\**\*.cs" 
			target="src\Cirrious.MvvmCross.Plugins.DownloadCache" />
		<file src="..\Plugins\Cirrious\DownloadCache\Cirrious.MvvmCross.Plugins.DownloadCache.Droid\**\*.cs" 
			target="src\Cirrious.MvvmCross.Plugins.DownloadCache.Droid" />
		<file src="..\Plugins\Cirrious\DownloadCache\Cirrious.MvvmCross.Plugins.DownloadCache.Touch\**\*.cs" 
			target="src\Cirrious.MvvmCross.Plugins.DownloadCache.Touch" />	
			
		<!-- PCL -->
		<file src="..\bin\Release\Mvx\Portable\Cirrious.MvvmCross.Plugins.DownloadCache.*" target="lib\portable-win+net45+wp8+win8+wpa81" />
		
        
		<!-- droid -->
		<file src="..\bin\Release\Mvx\Portable\Cirrious.MvvmCross.Plugins.DownloadCache.*" target="lib\MonoAndroid" />
		<file src="..\bin\Release\Mvx\Droid\Cirrious.MvvmCross.Plugins.DownloadCache.Droid.*" target="lib\MonoAndroid" />
        <file src="BootstrapContent\DownloadCachePluginBootstrap.cs.pp" target="content\MonoAndroid\Bootstrap\DownloadCachePluginBootstrap.cs.pp" />

		<!-- touch -->
		<file src="..\bin\Release\Mvx\Portable\Cirrious.MvvmCross.Plugins.DownloadCache.*" target="lib\Xamarin.iOS10" />
		<file src="..\bin\Release\Mvx\Touch\Cirrious.MvvmCross.Plugins.DownloadCache.Touch.*" target="lib\Xamarin.iOS10" />
        <file src="TouchBootstrapContent\DownloadCachePluginBootstrap.cs.pp" target="content\Xamarin.iOS10\Bootstrap\DownloadCachePluginBootstrap.cs.pp" />
	
	</files>
</package>







