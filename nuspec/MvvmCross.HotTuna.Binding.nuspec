<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
	<metadata>
		<id>MvvmCross.HotTuna.Binding</id>
		<version>4.0.0-beta4</version>
		<title>MvvmCross - Binding</title>
		<authors>MvvmCross</authors>
		<owners>Stuart Lodge</owners>
		<licenseUrl>http://opensource.org/licenses/ms-pl.html</licenseUrl>
		<projectUrl>https://github.com/MvvmCross/MvvmCross</projectUrl>
		<requireLicenseAcceptance>false</requireLicenseAcceptance>
		<description>This package contains the 'Core Binding' libraries for MvvmCross</description>
		<tags>mvvm mvvmcross xamarin monodroid Xamarin.iOS10 wpf windows8 winrt netcore wp wpdev windowsphone</tags>
		<iconUrl>http://i.imgur.com/BvdAtgT.png</iconUrl>
		<frameworkAssemblies>
			<frameworkAssembly assemblyName="Mono.Android.Export" targetFramework="MonoAndroid" />
		</frameworkAssemblies>
	</metadata>
	<files>		
		<!-- pcl -->
		<file src="..\bin\Release\Mvx\Portable\Cirrious.MvvmCross.Binding.*" target="lib\portable-win+net45+wp8+win8+wpa81" />

		<!-- dotnet -->
		<file src="..\bin\Release\Mvx\Portable\Cirrious.MvvmCross.Binding.*" target="lib\dotnet" />
		
		<!-- droid -->
		<file src="..\bin\Release\Mvx\Droid\Cirrious.MvvmCross.Binding.Droid.*" target="lib\MonoAndroid" />
		<file src="..\bin\Release\Mvx\Portable\Cirrious.MvvmCross.Binding.*" target="lib\MonoAndroid" />

		<!-- touch -->
		<file src="..\bin\Release\Mvx\Touch\Cirrious.MvvmCross.Binding.Touch.*" target="lib\Xamarin.iOS10" />
		<file src="..\bin\Release\Mvx\Portable\Cirrious.MvvmCross.Binding.*" target="lib\Xamarin.iOS10" />

		<!-- phone -->
		<file src="..\bin\Release\Mvx\Portable\Cirrious.MvvmCross.Binding.*" 
			target="lib\wp8" />

		<!-- store - pcl -->
		<file src="..\bin\Release\Mvx\Portable\Cirrious.MvvmCross.Binding.*" 
			target="lib\netcore45" />

		<!-- wpf - pcl -->
		<file src="..\bin\Release\Mvx\Portable\Cirrious.MvvmCross.Binding.*" 
			target="lib\net45" />	

		<!-- src -->
		<file src="..\Cirrious\Cirrious.MvvmCross.Binding\**\*.cs" target="src\Cirrious.MvvmCross.Binding" />
		<file src="..\Cirrious\Cirrious.MvvmCross.Binding.Droid\**\*.cs" target="src\Cirrious.MvvmCross.Binding.Droid" />
		<file src="..\Cirrious\Cirrious.MvvmCross.Binding.Touch\**\*.cs" target="src\Cirrious.MvvmCross.Binding.Touch" />

	</files>
</package>





