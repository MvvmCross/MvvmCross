		<dependencies>
			<group targetFramework ="MonoAndroid">
				<dependency id="MvvmCross.HotTuna.AutoViews" version="4.0.0-beta7" />
				<dependency id="MvvmCross.HotTuna.Droid.Dialog" version="4.0.0-beta7" />
			</group>
			<group targetFramework ="dotnet">
				<dependency id="MvvmCross.HotTuna.AutoViews" version="4.0.0-beta7" />
				<dependency id="MvvmCross.HotTuna.Droid.Dialog" version="4.0.0-beta7" />
			</group>
		</dependencies>
	</metadata>
	<files>		
		<!-- droid -->
		<file src="..\bin\Release\Mvx\Droid\Cirrious.MvvmCross.AutoView.Droid.*" target="lib\MonoAndroid" />
		<file src="..\Cirrious\Cirrious.MvvmCross.AutoView.Droid\**\*.cs" target="src\Cirrious.MvvmCross.AutoView.Droid" />
		<file src="..\bin\Release\Mvx\Droid\CrossUI.Droid.*" target="lib\MonoAndroid" />
		<file src="..\CrossUI\CrossUI.Droid\**\*.cs" target="src\CrossUI.Droid"/>
	</files>
</package>








