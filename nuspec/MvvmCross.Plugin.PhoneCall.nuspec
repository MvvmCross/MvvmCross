<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
	<metadata>
		<id>MvvmCross.Plugin.PhoneCall</id>
		<version>4.4.0</version>
		<title>MvvmCross - PhoneCall Plugin</title>
		<authors>MvvmCross</authors>
		<owners>Stuart Lodge</owners>
		<licenseUrl>http://opensource.org/licenses/ms-pl.html</licenseUrl>
		<projectUrl>https://github.com/MvvmCross/MvvmCross-Plugins</projectUrl>
		<requireLicenseAcceptance>false</requireLicenseAcceptance>
		<description>MvvmCross is the .NET MVVM framework for cross-platform solutions, including Xamarin iOS, Xamarin Android, Xamarin Forms, Windows and Mac.

This package contains the 'PhoneCall' plugin for MvvmCross</description>
		<tags>mvvm mvvmcross cross xamarin android ios forms monodroid monotouch xamarin.android xamarin.ios xamarin.forms wpf windows8 winrt net net45 netcore wp wpdev windowsphone windowsstore uwp plugin</tags>
		<iconUrl>http://i.imgur.com/BvdAtgT.png</iconUrl>
		<dependencies>
			<dependency id="MvvmCross.Platform" version="4.4.0" />
		</dependencies>
	</metadata>
	<files>		
		<!-- src -->
		<file src="..\PhoneCall\MvvmCross.Plugins.PhoneCall\**\*.cs" 
			target="src\MvvmCross.Plugins.PhoneCall" />
		<file src="..\PhoneCall\MvvmCross.Plugins.PhoneCall.WindowsStore\**\*.cs" 
			target="src\MvvmCross.Plugins.PhoneCall.WindowsStore" />
		<file src="..\PhoneCall\MvvmCross.Plugins.PhoneCall.WindowsCommon\**\*.cs" 
			target="src\MvvmCross.Plugins.PhoneCall.WindowsCommon" />
		<file src="..\PhoneCall\MvvmCross.Plugins.PhoneCall.Droid\**\*.cs" 	
			target="src\MvvmCross.Plugins.PhoneCall.Droid" />
		<file src="..\PhoneCall\MvvmCross.Plugins.PhoneCall.iOS\**\*.cs" 
			target="src\MvvmCross.Plugins.PhoneCall.iOS" />

		<!-- PCL -->
		<file src="..\bin\Release\Mvx\Portable\MvvmCross.Plugins.PhoneCall.*" target="lib\portable-net45+win+wpa81+wp80" />
		
		<!-- dotnet -->
		<file src="..\bin\Release\Mvx\Portable\MvvmCross.Plugins.PhoneCall.*" target="lib\dotnet" />

		<!-- store -->
		<file src="..\bin\Release\Mvx\Portable\MvvmCross.Plugins.PhoneCall.*" target="lib\netcore45" />
		<file src="..\bin\Release\Mvx\WindowsStore\MvvmCross.Plugins.PhoneCall.WindowsStore.*" target="lib\netcore45" />
		<file src="BootstrapContent\PhoneCallPluginBootstrap.cs.pp" target="content\netcore45\Bootstrap\PhoneCallPluginBootstrap.cs.pp" />

		<!-- windows common -->
		<file src="..\bin\Release\Mvx\Portable\MvvmCross.Plugins.PhoneCall.*" target="lib\win81" />
		<file src="..\bin\Release\Mvx\WindowsStore\MvvmCross.Plugins.PhoneCall.WindowsStore.*" target="lib\win81" />
		<file src="BootstrapContent\PhoneCallPluginBootstrap.cs.pp" target="content\win81\Bootstrap\PhoneCallPluginBootstrap.cs.pp" />

		<file src="..\bin\Release\Mvx\Portable\MvvmCross.Plugins.PhoneCall.*" target="lib\wpa81" />
		<file src="..\bin\Release\Mvx\WindowsPhoneStore\MvvmCross.Plugins.PhoneCall.WindowsPhoneStore.*" target="lib\wpa81" />
		<file src="BootstrapContent\PhoneCallPluginBootstrap.cs.pp" target="content\wpa81\Bootstrap\PhoneCallPluginBootstrap.cs.pp" />

		<!-- droid -->
		<file src="..\bin\Release\Mvx\Portable\MvvmCross.Plugins.PhoneCall.*" target="lib\MonoAndroid" />
		<file src="..\bin\Release\Mvx\Droid\MvvmCross.Plugins.PhoneCall.Droid.*" target="lib\MonoAndroid" />
		<file src="BootstrapContent\PhoneCallPluginBootstrap.cs.pp" target="content\MonoAndroid\Bootstrap\PhoneCallPluginBootstrap.cs.pp" />

		<!-- iOS -->
		<file src="..\bin\Release\Mvx\Portable\MvvmCross.Plugins.PhoneCall.*" target="lib\Xamarin.iOS10" />
		<file src="..\bin\Release\Mvx\iOS\MvvmCross.Plugins.PhoneCall.iOS.*" target="lib\Xamarin.iOS10" />

		<file src="iOSBootstrapContent\PhoneCallPluginBootstrap.cs.pp" target="content\Xamarin.iOS10\Bootstrap\PhoneCallPluginBootstrap.cs.pp" />
		
		<!-- mac -->
		<file src="..\bin\Release\Mvx\Portable\MvvmCross.Plugins.PhoneCall.*" target="lib\Xamarin.Mac20" />

	</files>
</package>


















