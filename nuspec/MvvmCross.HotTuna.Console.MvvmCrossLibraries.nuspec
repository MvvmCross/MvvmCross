				<dependency id="MvvmCross.HotTuna.CrossCore" version="4.0.0-beta3" />
			</group>
			<group targetFramework ="dotnet">
				<dependency id="MvvmCross.HotTuna.CrossCore" version="4.0.0-beta3" />
			</group>
