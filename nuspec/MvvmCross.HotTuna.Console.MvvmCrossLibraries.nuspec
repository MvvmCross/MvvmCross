		<!-- console -->
		<file src="..\bin\Release\Mvx\Console\Cirrious.MvvmCross.Console.*" target="lib\net45" />

		<!-- console - PCL -->
		<file src="..\bin\Release\Mvx\Portable\Cirrious.MvvmCross.dll" target="lib\net45\Cirrious.MvvmCross.dll" />
		<file src="..\bin\Release\Mvx\Portable\Cirrious.MvvmCross.pdb" target="lib\net45\Cirrious.MvvmCross.pdb" />
