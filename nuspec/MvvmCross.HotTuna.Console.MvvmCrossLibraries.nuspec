	<files>		
		<!-- PCL -->
		<file src="..\bin\Release\Mvx\Portable\Cirrious.MvvmCross.*" target="lib\portable-win+net45+wp8+win8+wpa81+Xamarin.iOS10+MonoAndroid" />

		<!-- dotnet -->
