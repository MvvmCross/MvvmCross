<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
	<metadata>
		<id>MvvmCross.HotTuna.Plugin.File</id>
		<version>4.0.0-alpha8</version>
		<title>MvvmCross - File Plugin</title>
		<authors>MvvmCross</authors>
		<owners>Stuart Lodge</owners>
		<licenseUrl>http://opensource.org/licenses/ms-pl.html</licenseUrl>
		<projectUrl>https://github.com/slodge/MvvmCross</projectUrl>
		<requireLicenseAcceptance>false</requireLicenseAcceptance>
		<description>This package contains the 'File' plugin for MvvmCross v3 - Hot Tuna. This and other packages are currently under development...</description>
		<tags>mvvm mvvmcross xamarin monodroid Xamarin.iOS10 wpf windows8 winrt netcore wp wpdev windowsphone</tags>
		<dependencies>
		  <dependency id="MvvmCross.HotTuna.CrossCore" version="4.0.0-alpha8" />
		</dependencies>
		<iconUrl>http://i.imgur.com/ymzMpLX.png</iconUrl>
