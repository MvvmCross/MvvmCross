		<file src="..\bin\Release\Mvx\Portable\Cirrious.MvvmCross.Plugins.File.*" target="lib\Xamarin.iOS10" />
		<file src="..\bin\Release\Mvx\Touch\Cirrious.MvvmCross.Plugins.File.Touch.*" target="lib\Xamarin.iOS10" />
        <file src="TouchBootstrapContent\FilePluginBootstrap.cs.pp" target="content\Xamarin.iOS10\Bootstrap\FilePluginBootstrap.cs.pp" />
