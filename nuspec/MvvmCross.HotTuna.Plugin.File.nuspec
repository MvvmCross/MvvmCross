		<file src="..\bin\Release\Mvx\Portable\Cirrious.MvvmCross.Plugins.File.*" target="lib\win81" />
		<file src="..\bin\Release\Mvx\WindowsCommon\Cirrious.MvvmCross.Plugins.File.WindowsCommon.*" target="lib\win81" />
        <file src="BootstrapContent\FilePluginBootstrap.cs.pp" target="content\win81\Bootstrap\FilePluginBootstrap.cs.pp" />

		<file src="..\bin\Release\Mvx\Portable\Cirrious.MvvmCross.Plugins.File.*" target="lib\wpa81" />
		<file src="..\bin\Release\Mvx\WindowsCommon\Cirrious.MvvmCross.Plugins.File.WindowsCommon.*" target="lib\wpa81" />
