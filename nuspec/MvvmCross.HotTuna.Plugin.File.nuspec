		<file src="..\Plugins\Cirrious\File\Cirrious.MvvmCross.Plugins.File\**\*.cs" 
			target="src\Cirrious.MvvmCross.Plugins.File" />
		<file src="..\Plugins\Cirrious\File\Cirrious.MvvmCross.Plugins.File.Wpf\**\*.cs" 
			target="src\Cirrious.MvvmCross.Plugins.File.Wpf" />
		<file src="..\Plugins\Cirrious\File\Cirrious.MvvmCross.Plugins.File.WindowsStore\**\*.cs" 
