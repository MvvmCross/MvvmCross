<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
	<metadata>
		<id>MvvmCross.Mac.Core</id>
		<version>4.0.0-beta8</version>
		<title>MvvmCross - Mac Core</title>
		<authors>MvvmCross</authors>
		<owners>Stuart Lodge</owners>
		<licenseUrl>http://opensource.org/licenses/ms-pl.html</licenseUrl>
		<projectUrl>https://github.com/MvvmCross/MvvmCross</projectUrl>
		<requireLicenseAcceptance>false</requireLicenseAcceptance>
		<description>
		MvvmCross is the .NET MVVM framework for cross-platform solutions, including Xamarin iOS, Xamarin Android, Xamarin Forms, Windows and Mac.

This package contains Xamarin.Mac (also PCL, MonoAndroid and Xamarin.iOS10 for convenience) 'Core' assemblies for MvvmCross. Under normal circumstances, use MvvmCross.Mac.PortableSupport for PCL, Xamarin.iOS10 and MonoAndroid as it also includes Windows.
		</description>
		<tags>mvvm mvvmcross cross xamarin android ios forms monodroid monotouch xamarin.android xamarin.ios xamarin.forms wpf windows8 winrt net net45 netcore wp wpdev windowsphone windowsstore uwp</tags>
		<iconUrl>http://i.imgur.com/BvdAtgT.png</iconUrl>
	</metadata>
	<files>
		<!-- Common -->
		<file src="..\MvvmCross\Core\Core\**\*.cs" target="src\MvvmCross.Core" />
		<file src="..\MvvmCross\Core\Localization\**\*.cs" target="src\MvvmCross.Localization" />
		<file src="..\MvvmCross\Core\Binding\**\*.cs" target="src\MvvmCross.Binding" />

		<!-- PCL -->
		<file src="..\bin\Release\Mvx\Portable\MvvmCross.Core.*" target="lib\portable-net45+win+wpa81+wp80" />
		<file src="..\bin\Release\Mvx\Portable\MvvmCross.Localization.*" target="lib\portable-net45+win+wpa81+wp80" />

		<!-- dotnet -->
		<file src="..\bin\Release\Mvx\Portable\MvvmCross.Core.*" target="lib\dotnet" />
		<file src="..\bin\Release\Mvx\Portable\MvvmCross.Localization.*" target="lib\dotnet" />

		<!-- droid -->
		<file src="..\bin\Release\Mvx\Droid\MvvmCross.Droid.*" target="lib\MonoAndroid" />
		<file src="..\MvvmCross\Droid\Droid\**\*.cs" target="src\MvvmCross.Droid" />
		<file src="..\bin\Release\Mvx\Droid\MvvmCross.Binding.Droid.*" target="lib\MonoAndroid" />
		<file src="..\MvvmCross\Binding\Droid\**\*.cs" target="src\MvvmCross.Binding.Droid" />

		<!-- droid - PCL -->
		<file src="..\bin\Release\Mvx\Portable\MvvmCross.Core.*" target="lib\MonoAndroid" />
		<file src="..\bin\Release\Mvx\Portable\MvvmCross.Binding.*" target="lib\MonoAndroid" />
		<file src="..\bin\Release\Mvx\Portable\MvvmCross.Localization.*" target="lib\MonoAndroid" />

		<!-- iOS -->
		<file src="..\bin\Release\Mvx\iOS\MvvmCross.iOS.*" target="lib\Xamarin.iOS10" />
		<file src="..\MvvmCross\iOS\iOS\**\*.cs" target="src\MvvmCross.iOS" />
		<file src="..\bin\Release\Mvx\iOS\MvvmCross.Binding.iOS.*" target="lib\Xamarin.iOS10" />
		<file src="..\MvvmCross\Binding\iOS\**\*.cs" target="src\MvvmCross.Binding.iOS" />

		<!-- iOS - PCL -->
		<file src="..\bin\Release\Mvx\Portable\MvvmCross.Core.*" target="lib\Xamarin.iOS10" />
		<file src="..\bin\Release\Mvx\Portable\MvvmCross.Binding.*" target="lib\Xamarin.iOS10" />
		<file src="..\bin\Release\Mvx\Portable\MvvmCross.Localization.*" target="lib\Xamarin.iOS10" />

		<!-- mac -->
		<file src="..\bin\Release\Mvx\Mac\MvvmCross.Mac.*" target="lib\Xamarin.Mac20" />
		<file src="..\MvvmCross\Mac\Mac\**\*.cs" target="src\MvvmCross.Mac" />
		<file src="..\bin\Release\Mvx\Mac\MvvmCross.Binding.Mac.*" target="lib\Xamarin.Mac20" />
		<file src="..\MvvmCross\Binding\Mac\**\*.cs" target="src\MvvmCross.Binding.Mac" />

		<!-- mac - PCL -->
		<file src="..\bin\Release\Mvx\Portable\MvvmCross.Core.*" target="lib\net45" />
		<file src="..\bin\Release\Mvx\Portable\MvvmCross.Binding.*" target="lib\net45" />
		<file src="..\bin\Release\Mvx\Portable\MvvmCross.Localization.*" target="lib\net45" />

	</files>
</package>








