<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
	<metadata>
		<id>MvvmCross.HotTuna.MvvmCrossLibraries</id>
		<version>4.0.0-beta2</version>
		<title>MvvmCross - MvvmCross Libraries</title>
		<authors>MvvmCross</authors>
		<owners>Stuart Lodge</owners>
		<licenseUrl>http://opensource.org/licenses/ms-pl.html</licenseUrl>
		<projectUrl>https://github.com/MvvmCross/MvvmCross</projectUrl>
		<requireLicenseAcceptance>false</requireLicenseAcceptance>
		<description>This package contains the 'MvvmCross Library' libraries for MvvmCross</description>
		<iconUrl>http://i.imgur.com/ymzMpLX.png</iconUrl>
		<dependencies>
			<group targetFramework ="net">
				<dependency id="MvvmCross.HotTuna.CrossCore" version="4.0.0-beta2" />
			</group>
			<group targetFramework ="win">
				<dependency id="MvvmCross.HotTuna.CrossCore" version="4.0.0-beta2" />
			</group>
			<group targetFramework ="wp">
				<dependency id="MvvmCross.HotTuna.CrossCore" version="4.0.0-beta2" />
			</group>
			<group targetFramework ="wpa">
				<dependency id="MvvmCross.HotTuna.CrossCore" version="4.0.0-beta2" />
			</group>
			<group targetFramework ="MonoAndroid">
				<dependency id="MvvmCross.HotTuna.CrossCore" version="4.0.0-beta2" />
			</group>
			<group targetFramework ="MonoTouch">
				<dependency id="MvvmCross.HotTuna.CrossCore" version="4.0.0-beta2" />
			</group>
			<group targetFramework ="Xamarin.iOS">
				<dependency id="MvvmCross.HotTuna.CrossCore" version="4.0.0-beta2" />
			</group>
			<group targetFramework ="portable-net45+win8+wpa81+wp8">
				<dependency id="MvvmCross.HotTuna.CrossCore" version="4.0.0-beta2" />
			</group>
			<group targetFramework ="uap">
				<dependency id="MvvmCross.HotTuna.CrossCore" version="4.0.0-beta2" />
			</group>
			<group targetFramework ="dotnet">
				<dependency id="MvvmCross.HotTuna.CrossCore" version="4.0.0-beta2" />
			</group>
		</dependencies>
	</metadata>
	<files>		
		<!-- src -->
		<file src="..\Cirrious\Cirrious.MvvmCross\**\*.cs" 
			target="src\Cirrious.MvvmCross" />
		<file src="..\Cirrious\Cirrious.MvvmCross.Wpf\**\*.cs" 
			target="src\Cirrious.MvvmCross.Wpf" />
		<file src="..\Cirrious\Cirrious.MvvmCross.WindowsStore\**\*.cs" 
			target="src\Cirrious.MvvmCross.WindowsStore" />
		<file src="..\Cirrious\Cirrious.MvvmCross.WindowsPhone\**\*.cs" 
			target="src\Cirrious.MvvmCross.WindowsPhone" />
		<file src="..\Cirrious\Cirrious.MvvmCross.WindowsCommon\**\*.cs" 
			target="src\Cirrious.MvvmCross.WindowsCommon" />
		<file src="..\Cirrious\Cirrious.MvvmCross.WindowsUWP\**\*.cs" 
			target="src\Cirrious.MvvmCross.WindowsUWP" />
		<file src="..\Cirrious\Cirrious.MvvmCross.Droid\**\*.cs" 
			target="src\Cirrious.MvvmCross.Droid" />
		<file src="..\Cirrious\Cirrious.MvvmCross.Touch\**\*.cs" 
			target="src\Cirrious.MvvmCross.Touch" />

		<!-- PCL -->
		<file src="..\bin\Release\Mvx\Portable\Cirrious.MvvmCross.dll" target="lib\portable-win+net45+wp8+win8+wpa81" />
		<file src="..\bin\Release\Mvx\Portable\Cirrious.MvvmCross.pdb" target="lib\portable-win+net45+wp8+win8+wpa81" />

		<!-- dotnet -->
		<file src="..\bin\Release\Mvx\Portable\Cirrious.MvvmCross.dll" target="lib\dotnet" />
		<file src="..\bin\Release\Mvx\Portable\Cirrious.MvvmCross.pdb" target="lib\dotnet" />

		<!-- wpf -->
		<file src="..\bin\Release\Mvx\Wpf\Cirrious.MvvmCross.Wpf.*" target="lib\net45" />
		<file src="..\bin\Release\Mvx\Portable\Cirrious.MvvmCross.dll" target="lib\net45" />
		<file src="..\bin\Release\Mvx\Portable\Cirrious.MvvmCross.pdb" target="lib\net45" />

		<!-- store -->
		<file src="..\bin\Release\Mvx\WindowsStore\Cirrious.MvvmCross.WindowsStore.*" target="lib\netcore45" />
		<file src="..\bin\Release\Mvx\Portable\Cirrious.MvvmCross.dll" target="lib\netcore45" />
		<file src="..\bin\Release\Mvx\Portable\Cirrious.MvvmCross.pdb" target="lib\netcore45" />

		<!-- phone -->
		<file src="..\bin\Release\Mvx\WindowsPhone\Cirrious.MvvmCross.WindowsPhone.*" target="lib\wp8" />
		<file src="..\bin\Release\Mvx\Portable\Cirrious.MvvmCross.dll" target="lib\wp8" />
		<file src="..\bin\Release\Mvx\Portable\Cirrious.MvvmCross.pdb" target="lib\wp8" />

		<!-- windows common -->
		<file src="..\bin\Release\Mvx\WindowsCommon\Cirrious.MvvmCross.WindowsCommon.*" target="lib\win81" />
		<file src="..\bin\Release\Mvx\WindowsCommon\Cirrious.MvvmCross.WindowsCommon.*" target="lib\wpa81" />
		<file src="..\bin\Release\Mvx\WindowsCommon\Cirrious.MvvmCross.WindowsCommon.*" target="lib\portable-win81+wpa81" />
		<file src="..\bin\Release\Mvx\Portable\Cirrious.MvvmCross.dll" target="lib\win81" />
		<file src="..\bin\Release\Mvx\Portable\Cirrious.MvvmCross.pdb" target="lib\win81" />
		<file src="..\bin\Release\Mvx\Portable\Cirrious.MvvmCross.dll" target="lib\wpa81" />
		<file src="..\bin\Release\Mvx\Portable\Cirrious.MvvmCross.pdb" target="lib\wpa81" />
		<file src="..\bin\Release\Mvx\Portable\Cirrious.MvvmCross.dll" target="lib\portable-win81+wpa81" />
		<file src="..\bin\Release\Mvx\Portable\Cirrious.MvvmCross.pdb" target="lib\portable-win81+wpa81" />

		<!-- uap -->
		<file src="..\bin\Release\Mvx\WindowsUWP\Cirrious.MvvmCross.WindowsUWP.*" target="lib\uap" />
		<file src="..\bin\Release\Mvx\Portable\Cirrious.MvvmCross.dll" target="lib\uap" />
		<file src="..\bin\Release\Mvx\Portable\Cirrious.MvvmCross.pdb" target="lib\uap" />
		
		<!-- droid -->
		<file src="..\bin\Release\Mvx\Droid\Cirrious.MvvmCross.Droid.*"
			exclude="..\bin\Release\Mvx\Droid\Cirrious.MvvmCross.Droid.Full*" target="lib\MonoAndroid" />
		<file src="..\bin\Release\Mvx\Portable\Cirrious.MvvmCross.dll" target="lib\MonoAndroid" />
		<file src="..\bin\Release\Mvx\Portable\Cirrious.MvvmCross.pdb" target="lib\MonoAndroid" />

		<!-- touch -->
		<file src="..\bin\Release\Mvx\Touch\Cirrious.MvvmCross.Touch.*" target="lib\Xamarin.iOS10" />
		<file src="..\bin\Release\Mvx\Portable\Cirrious.MvvmCross.dll" target="lib\Xamarin.iOS10" />
		<file src="..\bin\Release\Mvx\Portable\Cirrious.MvvmCross.pdb" target="lib\Xamarin.iOS10" />
	</files>
</package>







