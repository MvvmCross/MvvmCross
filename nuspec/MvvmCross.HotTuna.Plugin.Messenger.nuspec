<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
	<metadata>
		<id>MvvmCross.HotTuna.Plugin.Messenger</id>
		<version>4.0.0-beta1</version>
		<title>MvvmCross - Messenger Plugin</title>
		<authors>MvvmCross</authors>
		<owners>Stuart Lodge</owners>
		<licenseUrl>http://opensource.org/licenses/ms-pl.html</licenseUrl>
		<projectUrl>https://github.com/MvvmCross/MvvmCross</projectUrl>
		<requireLicenseAcceptance>false</requireLicenseAcceptance>
		<description>This package contains the 'Messenger' plugin for MvvmCross</description>
		<tags>mvvm mvvmcross xamarin monodroid Xamarin.iOS10 wpf windows8 winrt netcore wp wpdev windowsphone</tags>
		<iconUrl>http://i.imgur.com/ymzMpLX.png</iconUrl>
		<dependencies>
			<group targetFramework ="net">
				<dependency id="MvvmCross.HotTuna.CrossCore" version="4.0.0-beta1" />
			</group>
			<group targetFramework ="win">
				<dependency id="MvvmCross.HotTuna.CrossCore" version="4.0.0-beta1" />
			</group>
			<group targetFramework ="wp">
				<dependency id="MvvmCross.HotTuna.CrossCore" version="4.0.0-beta1" />
			</group>
			<group targetFramework ="wpa">
				<dependency id="MvvmCross.HotTuna.CrossCore" version="4.0.0-beta1" />
			</group>
			<group targetFramework ="MonoAndroid">
				<dependency id="MvvmCross.HotTuna.CrossCore" version="4.0.0-beta1" />
			</group>
			<group targetFramework ="MonoTouch">
				<dependency id="MvvmCross.HotTuna.CrossCore" version="4.0.0-beta1" />
			</group>
			<group targetFramework ="Xamarin.iOS">
				<dependency id="MvvmCross.HotTuna.CrossCore" version="4.0.0-beta1" />
			</group>
			<group targetFramework ="portable-net45+win8+wpa81+wp8">
				<dependency id="MvvmCross.HotTuna.CrossCore" version="4.0.0-beta1" />
			</group>
			<group targetFramework ="uap">
				<dependency id="MvvmCross.HotTuna.CrossCore" version="4.0.0-beta1" />
			</group>
			<group targetFramework ="dotnet">
				<dependency id="MvvmCross.HotTuna.CrossCore" version="4.0.0-beta1" />
			</group>
		</dependencies>
	</metadata>
	<files>
		<!-- Common -->
		<file src="..\Plugins\Cirrious\Messenger\Cirrious.MvvmCross.Plugins.Messenger\**\*.cs" 
			target="src\Cirrious.MvvmCross.Plugins.Messenger" />

		<!-- PCL -->
		<file src="..\bin\Release\Mvx\Portable\Cirrious.MvvmCross.Plugins.Messenger.*" target="lib\portable-win+net45+wp8+win8+wpa81" />

		<!-- wpf -->
		<file src="..\bin\Release\Mvx\Portable\Cirrious.MvvmCross.Plugins.Messenger.*" target="lib\net45" />
		<file src="BootstrapContent\MessengerPluginBootstrap.cs.pp" target="content\net45\Bootstrap\MessengerPluginBootstrap.cs.pp" />

		<!-- store -->
		<file src="..\bin\Release\Mvx\Portable\Cirrious.MvvmCross.Plugins.Messenger.*" target="lib\netcore45" />
		<file src="BootstrapContent\MessengerPluginBootstrap.cs.pp" target="content\netcore45\Bootstrap\MessengerPluginBootstrap.cs.pp" />

		<!-- phone -->
		<file src="..\bin\Release\Mvx\Portable\Cirrious.MvvmCross.Plugins.Messenger.*" target="lib\wp8" />
		<file src="BootstrapContent\MessengerPluginBootstrap.cs.pp" target="content\wp8\Bootstrap\MessengerPluginBootstrap.cs.pp" />

		<!-- windows common -->
		<file src="..\bin\Release\Mvx\Portable\Cirrious.MvvmCross.Plugins.Messenger.*" target="lib\win81" />
		<file src="BootstrapContent\MessengerPluginBootstrap.cs.pp" target="content\win81\Bootstrap\MessengerPluginBootstrap.cs.pp" />

		<file src="..\bin\Release\Mvx\Portable\Cirrious.MvvmCross.Plugins.Messenger.*" target="lib\wpa81" />
		<file src="BootstrapContent\MessengerPluginBootstrap.cs.pp" target="content\wpa81\Bootstrap\MessengerPluginBootstrap.cs.pp" />

		<file src="..\bin\Release\Mvx\Portable\Cirrious.MvvmCross.Plugins.Messenger.*" target="lib\portable-win81+wpa81" />

		<!-- droid -->
		<file src="..\bin\Release\Mvx\Portable\Cirrious.MvvmCross.Plugins.Messenger.*" target="lib\MonoAndroid" />
		<file src="BootstrapContent\MessengerPluginBootstrap.cs.pp" target="content\MonoAndroid\Bootstrap\MessengerPluginBootstrap.cs.pp" />

		<!-- touch -->
		<file src="..\bin\Release\Mvx\Portable\Cirrious.MvvmCross.Plugins.Messenger.*" target="lib\Xamarin.iOS10" />
		<file src="TouchBootstrapContent\MessengerPluginBootstrap.cs.pp" target="content\Xamarin.iOS10\Bootstrap\MessengerPluginBootstrap.cs.pp" />
	</files>
</package>







