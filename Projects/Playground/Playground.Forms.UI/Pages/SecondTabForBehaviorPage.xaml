<?xml version="1.0" encoding="utf-8" ?>
<views:MvxContentPage x:TypeArguments="navigation:SecondTabForBehaviorViewModel" xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:d="http://xamarin.com/schemas/2014/forms/design"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:views="clr-namespace:MvvmCross.Forms.Views;assembly=MvvmCross.Forms"
             xmlns:navigation="clr-namespace:Playground.Core.ViewModels.Navigation;assembly=Playground.Core"
             mc:Ignorable="d"
             Title="Second Tab"
             x:Class="Playground.Forms.UI.Pages.SecondTabForBehaviorPage">
    <StackLayout>
        <Label Text="{Binding SampleModel.Message}" 
               FontSize="Title"
               HorizontalOptions="Center" />
        <Label Text="{Binding SampleModel.Value}" 
               FontSize="Title"
               HorizontalOptions="Center" />
    </StackLayout>
</views:MvxContentPage>